{
  "name": "travel_planner",
  "version": "1.0.0",
  "description": "Simple travel planner app using Google Maps, OpenWeather, and Skyscanner APIs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:with-db": "node server-db.js",
    "dev:with-db": "nodemon server-db.js",
    "start:prod": "set NODE_ENV=production && node server-production.js",
    "dev:prod": "set NODE_ENV=production && nodemon server-production.js",
    "setup:db": "node setup-database.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:migrate": "npx prisma migrate dev"
  },
  "keywords": [
    "travel",
    "planner",
    "google-maps",
    "openweather",
    "skyscanner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
